@article{cianciFUTURISMENGLISHAVANTGARDE1981,
  title =        {{{FUTURISM AND THE ENGLISH AVANT}}-{{GARDE}}: {{THE EARLY
                  POUND BETWEEN IMAGISM AND VORTICISM}}},
  volume =       6,
  issn =         {0171-5410},
  url =          {https://www.jstor.org/stable/43023218},
  shorttitle =   {{{FUTURISM AND THE ENGLISH AVANT}}-{{GARDE}}},
  abstract =     {Pound's conversion from Imagism to Vorticism in 1913-1914 has
                  been generally considered a turning-point in his career, and
                  recent studies have stressed the importance of Vorticism for a
                  true understanding of the Cantos. Pound's criticism of the
                  "static image" has been rightly emphasized. However, Futurism
                  which publicized the concept and practice of dynamism has been
                  ignored or quickly passed over as a revolutionary movement
                  which made an impact on Pound. Even recently literary
                  historiography has surprisingly accepted Pound's and Wyndham
                  Lewis's antifuturistic statements without questioning them.
                  Yet there is ample evidence that Marinetti's manifestos and
                  his London performances were crucial in Pound's militant
                  transformation. This essay, after briefly describing the
                  Futurists' sensational impression on London, and following
                  their echoes inside the English avant-garde (see sections I,
                  II and III), traces the presence of Futurist tenets in Pound's
                  pamphleteerings of 1913 and 1914 and in his "Contemporania"
                  poems. It comments on the Futurist-inspired concept of
                  Vorticism (see section IV) and on the historical reasons which
                  brought about a local avant-garde caught between revolution
                  and reaction (see section V and VI). One should no longer see
                  Futurism through the distorted lens of Vorticism. Though by no
                  means the only influence on Pound in 1913-1914, Futurism
                  appears to have been momentous in his development.},
  number =       1,
  journaltitle = {AAA: Arbeiten aus Anglistik und Amerikanistik},
  urldate =      {2019-10-02},
  date =         1981,
  pages =        {3-39},
  author =       {Cianci, Giovanni},
  file =
                  {/home/jon/Dokumentujo/Papers/zotero/storage/X37CEQW4/Cianci_1981_FUTURISM
                  AND THE ENGLISH AVANT-GARDE.pdf}
}

