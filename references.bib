@article{goldberg14_word2_explain,
  author =       {Goldberg, Yoav and Levy, Omer},
  title =        {Word2vec Explained: Deriving Mikolov Et Al.'s
                  Negative-Sampling Word-Embedding Method},
  journal =      {CoRR},
  year =         2014,
  url =          {http://arxiv.org/abs/1402.3722v1},
  abstract =     {The word2vec software of Tomas Mikolov and colleagues
                  (https://code.google.com/p/word2vec/ ) has gained a lot of
                  traction lately, and provides state-of-the-art word
                  embeddings. The learning models behind the software are
                  described in two research papers. We found the description of
                  the models in these papers to be somewhat cryptic and hard to
                  follow. While the motivations and presentation may be obvious
                  to the neural-networks language-modeling crowd, we had to
                  struggle quite a bit to figure out the rationale behind the
                  equations. This note is an attempt to explain equation (4)
