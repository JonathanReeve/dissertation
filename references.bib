@article{van_hulle_periodizing_2016,
  eprinttype =   {jstor},
  eprint =       {10.5325/style.50.2.0172},
  title =        {Periodizing {{Samuel Beckett}}'s {{Works}}: {{A
                  Stylochronometric Approach}}},
  volume =       50,
  issn =         {0039-4238},
  doi =          {10.5325/style.50.2.0172},
  shorttitle =   {Periodizing {{Samuel Beckett}}'s {{Works}}},
  abstract =     {We report the first analysis of Samuel Beckett's prose
                  writings using stylometry, or the quantitative study of
                  writing style, focusing on grammatical function words, a
                  linguistic category that has seldom been studied before in
                  Beckett studies. To these function words, we apply methods
                  from computational stylometry and model the stylistic
                  evolution in Beckett's oeuvre. Our analyses reveal a number of
                  discoveries that shed new light on existing periodizations in
                  the secondary literature, which commonly distinguish an
                  “early,” “middle,” and “late” period in Beckett's oeuvre. We
                  analyze Beckett's prose writings in both English and French,
                  demonstrating notable symmetries and asymmetries between both
                  languages. The analyses nuance the traditional three-part
                  periodization as they show the possibility of stylistic
                  relapses (disturbing the linearity of most periodizations) as
                  well as different turning points depending on the language of
                  the corpus, suggesting that Beckett's English oeuvre is not
                  identical to his French oeuvre in terms of patterns of
                  stylistic development.},
  number =       2,
  journaltitle = {Style},
  shortjournal = {Style},
  date =         2016,
  pages =        {172-202},
  author =       {van Hulle, Dirk and Kestemont, Mike},
  options =      {useprefix=true},
  file =         {/home/jon/Dokumentujo/Papers/zotero/storage/2B7TQXK4/van Hulle and
                  Kestemont - 2016 - Periodizing Samuel Beckett's Works A
                  Stylochronom.pdf}
