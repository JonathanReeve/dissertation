  keywords =     {bots,collaboration,distributed
                  cognition,ethnography,qualitative,social,trace
                  ethnography,wiki,wikipedia},
  author =       {Geiger, R. Stuart and Ribes, David},
  file =         {/home/jon/Dokumentujo/Papers/SOPS/Geiger_Ribes_2010_The Work of
